process {
    withName:prebagel {
        cpus=1
        container = "mpgagebioinformatics/bagel:53388ad"
        containerOptions = "-v ${params.samples_tsv}:${params.samples_tsv} -v ${params.output_bagel}:${params.output_bagel}"
    }
    withName:probagel {
        executor='local'
        queue="${params.queue}"
        cpus=2
        container = "mpgagebioinformatics/bagel:53388ad"
        containerOptions = "-v ${params.ouput_mageck_count}:${params.ouput_mageck_count} -v ${params.output_bagel}:${params.output_bagel}"
    }
}